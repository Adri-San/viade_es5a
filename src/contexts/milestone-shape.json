{
  "@context": {
    "foaf": "http://xmlns.com/foaf/0.1/",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#",
    "viade": "http://arquisoft.github.io/viadeSpec/",
    "schema": "http://schema.org/"
  },
  "shape": [
    {
      "prefix": "foaf",
      "predicate": "name",
      "object": "name",
      "literal": true,
      "type": "String"
    },
    {
      "prefix": "foaf",
      "predicate": "description",
      "object": "description",
      "literal": true,
      "type": "String"
    },
    {
      "prefix": "geo",
      "predicate": "location",
      "object": "distance",
      "literal": true,
      "type": "number"
    },
    {
      "prefix": "schema",
      "predicate": "elevation",
      "object": "slope",
      "literal": true,
      "type": "number"
    },
    {
      "prefix": "schema",
      "predicate": "latitude",
      "object": "latitude",
      "literal": true,
      "type": "number"
    },
    {
      "prefix": "schema",
      "predicate": "longitude",
      "object": "longitude",
      "literal": true,
      "type": "number"
    },
    {
      "prefix": "viade",
      "predicate": "order",
      "object": "order",
      "literal": true,
      "type": "number"
    }
  ]
}
