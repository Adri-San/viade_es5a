{
  "@context": {
    "viade": "http://arquisoft.github.io/viadeSpec/",
    "schema": "http://schema.org/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#",
    "bibo": "http://purl.org/ontology/bibo/",
    "terms": "http://purl.org/dc/terms/"
  },
  "shape": [
    {
      "prefix": "schema",
      "predicate": "name",
      "object": "name",
      "literal": true,
      "type": "String"
    },
    {
      "prefix": "schema",
      "predicate": "description",
      "object": "description",
      "literal": true,
      "type": "String"
    },
    {
      "prefix": "bibo",
      "predicate": "degree",
      "object": "rank",
      "literal": true,
      "type": "number"
    },
    {
      "prefix": "schema",
      "predicate": "author",
      "object": "createdBy",
      "type": "webId"
    },
    {
      "prefix": "schema",
      "predicate": "publishedDate",
      "object": "createdAt",
      "literal": true,
      "type": "Date"
    },
    {
      "prefix": "viade",
      "predicate": "point",
      "object": "milestones",
      "type": "Array"
    },
    {
      "prefix": "viade",
      "predicate": "hasComments",
      "object": "messages",
      "literal": false,
      "type": "Array"
    },
    {
      "prefix": "viade",
      "predicate": "hasMediaAttached",
      "object": "media",
      "literal": false,
      "type": "Array"
    }
  ]
}
